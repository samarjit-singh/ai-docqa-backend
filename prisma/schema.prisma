generator client {
  provider = "go run github.com/steebchen/prisma-client-go"
  output   = "../generated/prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Message {
  id      Int    @id @default(autoincrement())
  content String
  createdAt DateTime @default(now())
}
